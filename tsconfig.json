{
  "compilerOptions": {
    "module": "es2022",
    "target": "es2022",
    "lib": [
      "es2022",
      "DOM"
    ],
    "moduleResolution": "node",
    "outDir": "cheats",
    "plugins": [
      {
        "transform": "@zoltu/typescript-transformer-append-js-extension/output/index.js",
        "after": true
      }
    ],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "useUnknownInCatchVariables": false,
    "declaration": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/window.d.ts"
  ],
  "exclude": [
    "node_modules",
    "test",
    "out"
  ]
}